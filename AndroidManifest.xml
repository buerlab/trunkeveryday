                android:name="com.baidu.lbsapi.API_KEY"
                android:value="oPV2UsGPfFGqhCeSFkrn9V3T" />
        <meta-data
                android:name="UMENG_APPKEY"
                android:value="53c5184156240bb4720f0f39" >
        <meta-data android:value="Naked" android:name="UMENG_CHANNEL"/>
        </meta-data>

        <!-- ###################注册SDK使用的Activity###################### -->
        <!--分享编辑页-->
