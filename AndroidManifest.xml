        <activity android:name=".driver.activities.SetTrunkActivity"/>

        <activity android:name=".driver.activities.EditTrunkActivity"></activity>
        <activity
                android:name=".BaiduMapActivity"
                android:configChanges="orientation|keyboardHidden"
                android:screenOrientation="sensor" >
        </activity>

        <activity android:name=".activities.EditNickNameActivity"
                   android:theme="@style/custom_theme"
